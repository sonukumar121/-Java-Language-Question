import java.util.Scanner;
public class Main {
    public static void main(String[] args)
    {
        int a,b,sum;
        System.out.println("Enter the two number= ");
        Scanner obj= new Scanner(System.in);
        a= obj.nextInt();
        b= obj.nextInt();
        sum=a+b;
        System.out.println("The sum of two number is: "+sum);

    }
}
